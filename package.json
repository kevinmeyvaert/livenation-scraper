    "typescript",
    "puppeteer",
    "livenation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^22.13.4",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.7",
    "openai": "^4.85.3",
    "puppeteer": "^24.2.1",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "ts-node": "^10.9.2"
  }
}
